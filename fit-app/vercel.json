{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "VITE_OPENROUTER_API_KEY": "@vite_openrouter_api_key",
    "VITE_GROQ_API_KEY": "@vite_groq_api_key", 
    "VITE_GOOGLE_AI_API_KEY": "@vite_google_ai_key"
  }
}
